<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appeventovolley.appevento" >
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name="com.appeventovolley.appevento.MyApplication">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ReadData"
            android:label="Procurar Evento" >
        </activity>
        <activity
            android:name=".UpdateDeleteData"
            android:label="Atualizar Evento" >
        </activity>

        <activity
            android:name=".cadastro"
            android:label="Criar Evento" >
        </activity>
    </application>

</manifest>
